<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<bean id="Paciente" class="denis.tapia.Paciente">
		<constructor-arg value="Dennis"></constructor-arg>
		<constructor-arg value="Tapia"></constructor-arg>
		<constructor-arg value="25"></constructor-arg>
	</bean>

	<bean id="Profesor" class="denis.tapia.Profesor">
		<constructor-arg value="Jose"></constructor-arg>
		<constructor-arg value="Sanchez"></constructor-arg>
		<constructor-arg value="Profesor"></constructor-arg>
		<constructor-arg value="UCE"></constructor-arg>
	</bean>


</beans>